stats entity @a[score_SearchDropper_min=1] set SuccessCount @s SuccessCount
scoreboard players set @a[score_SearchDropper_min=1] SuccessCount 0


#Blaze Tools
execute @a[score_SearchDropper_min=1] ~ ~ ~ execute @e[type=Armor_Stand,tag=CustomCraftingTable,c=1,r=5] ~ ~ ~ testforblock ~ ~ ~ dropper -1 {Items:[{id:"minecraft:blaze_powder",Count:1b,Slot:1b},{id:"minecraft:blaze_powder",Count:1b,Slot:3b},{id:"minecraft:blaze_powder",Count:1b,Slot:5b},{id:"minecraft:blaze_powder",Count:1b,Slot:7b},{id:"minecraft:iron_ingot",Count:1b,Slot:4b}]}
execute @a[score_SearchDropper_min=1,score_SuccessCount_min=1] ~ ~ ~ execute @e[type=Armor_Stand,tag=CustomCraftingTable,c=1,r=5] ~ ~ ~ blockdata ~ ~ ~ {Items:[{id:"minecraft:iron_ingot",Slot:4b,Count:1b,tag:{display:{LocName:"item.blazeIngot.name"},ench:[{lvl:1s,id:1s}],HideFlags:1,ItemID:"Blaze_Ingot"}}]}
execute @a[score_SearchDropper_min=1,score_SuccessCount_min=1] ~ ~ ~ execute @e[type=Armor_Stand,tag=CustomCraftingTable,c=1,r=5] ~ ~ ~ scoreboard players tag @s add Success

